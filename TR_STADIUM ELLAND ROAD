void idle()
{
    if (!mouseDown);
    {
        xrot += 0.3f;
        yrot += 0.4f;
    }
glutPostRedisplay();
}

void mouse(int button, int state, int x , int y)
{

    if (button == GLUT_LEFT_BUTTON && state == GLUT_DOWN)
    {
        mouseDown = true;
        xdiff = x - yrot;
        ydiff = -y + xrot;
    }
    else
    mouseDown = false;
}

void mouseMotion(int x, int y)
{
    myinit();
    if(mouseDown)
    {
        yrot = x - xdiff;
        xrot = y + ydiff;

        glutPostRedisplay();
    }
}

void keyboard(unsigned char key, int x, int y)
{
    switch (key)
    {
    //zoom kedepan
    case 'w':
    case 'W':
        glTranslatef(0.0, 0.0, 3.0);
        break;
    //zoom kebelakang
    case 's':
    case 'S':
        glTranslatef(0.0, 0.0, -3.0);
        break;
    //geser kekanan
    case 'd':
    case 'D':
        glTranslatef(3.0, 0.0, 0.0);
        break;
    //geser kekiri
    case 'a':
    case 'A':
        glTranslatef(-3.0, 0.0, 0.0);
        break;
    //naik
    case 'n':
    case 'N':
        glTranslatef(0.0, 3.0, 0.0);
        break;
    //turun
    case 't':
    case 'T':
        glTranslatef(0.0, -3.0, 0.0);
        break;
    //rolling ke belakang
    case 'y':
    case 'Y':
        glRotatef(-3.0, 1.0, 0.0, 0.0);
        break;
    //rolling ke depan
    case 'g':
    case 'G':
        glRotatef(2.0, 1.0, 0.0, 0.0);
        break;
    //putar ke kanan
    case 'j':
    case 'J':
        glRotatef(2.0, 0.0, 1.0, 0.0);
        break;
    //putar ke kiri
    case 'f':
    case 'F':
        glRotatef(-2.0, 0.0, 1.0, 0.0);
        break;
     //roling ke kanan
    case 'k':
    case 'K':
        glRotatef(-3.0, 0.0, 0.0, 1.0);
        break;
    //roling ke kiri
    case 'h':
    case 'H':
        glRotatef(3.0, 0.0, 0.0, 1.0);
        break;
    //tembus pandang
    case 'q':
    case 'Q':
            if (is_depth)
            {
                is_depth = 0;
                glDisable(GL_DEPTH_TEST);
            }
            else
            {
                is_depth = 1;
                glEnable(GL_DEPTH_TEST);
            }
        }
        tampil();
}

void ukuran(int lebar, int tinggi)
{
    if (tinggi == 0) tinggi = 1;

    glMatrixMode(GL_PROJECTION);
    glLoadIdentity();
    gluPerspective(50.0, lebar / tinggi, 5.0, 500.0);
    glTranslatef(0.0, -5.0, -150.0);
    glMatrixMode(GL_MODELVIEW);
    glViewport(200, 150, lebar/2,tinggi/2);
}












